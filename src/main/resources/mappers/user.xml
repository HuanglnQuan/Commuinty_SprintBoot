<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lnquan.community.dao.UserDao">
    <insert id="addUser" parameterType="User">
        INSERT INTO USER(ACCOUNT_ID, NAME, TOKEN, GMT_CREATE, GMT_MODIFIED)
        VALUES (#{accountId}, #{name}, #{token}, #{gmtCreate}, #{gmtModified})
    </insert>

    <select id="queryByToken" parameterType="java.lang.String" resultType="User">
        SELECT * FROM USER WHERE TOKEN = #{token}
    </select>
</mapper>
